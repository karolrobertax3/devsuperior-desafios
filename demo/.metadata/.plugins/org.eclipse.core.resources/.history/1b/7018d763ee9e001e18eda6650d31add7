package com.devsuperior.dseventos.entities;

public class Participante {

	private Integer id;
	
	private String nome;
	
	private String email;
	
	
	public Participante() {
		super();
		
	}
	public Participante(Integer id, String nome, String email) {
		super();
		this.id = id;
		this.nome = nome;
		this.email = email;
	}
	
	
}
